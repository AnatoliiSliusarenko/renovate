            <li {% if app.request.attributes.get('_route') == 'renovate_main_homepage' %}class="active"{% endif %}><a href="{{ path('renovate_main_homepage') }}">Головна</a></li>
            <li {% if app.request.attributes.get('_route') == 'renovate_tariffs' %}class="active"{% endif %}><a href="{{ path('renovate_tariffs') }}">Тарифи</a></li>
            <li {% if app.request.attributes.get('_route') == 'renovate_jobs' %}class="active"{% endif %}><a href="{{ path('renovate_jobs') }}">Види робіт</a></li>
            <li {% if app.request.attributes.get('_route') == 'renovate_results' %}class="active"{% endif %}><a href="{{ path('renovate_results') }}">Результати роботи</a></li>
            <li {% if app.request.attributes.get('_route') == 'renovate_news' %}class="active"{% endif %}><a href="{{ path('renovate_news') }}">Новини</a></li>
            <li {% if app.request.attributes.get('_route') == 'renovate_articles' %}class="active"{% endif %}><a href="{{ path('renovate_articles') }}">Статті</a></li>
            <li {% if app.request.attributes.get('_route') == 'renovate_shares' %}class="active"{% endif %}><a href="{{ path('renovate_shares') }}">Акції</a></li>
            <li {% if app.request.attributes.get('_route') == 'renovate_aboutus' %}class="active"{% endif %}><a href="{{ path('renovate_aboutus') }}">Про нас</a></li>
            <li {% if app.request.attributes.get('_route') == 'renovate_contacts' %}class="active"{% endif %}><a href="{{ path('renovate_contacts') }}">Контакти</a></li>
          </ul>
          <form class="navbar-form navbar-right">
            <div class="form-group">
              <input type="text" placeholder="Введіть запит..." class="form-control">
            </div>
            <button type="submit" class="btn btn-info disabled">ПОШУК!</button>
          </form>
        </div><!--/.nav-collapse -->
      </div>
    </nav>

    <div class="container" ng-app="Renovate">
		{% block breadcrumb %}{% endblock %}
	
	  	{% block content %}{% endblock %}
	  	<div class="row" style="margin-top:10px;" ng-controller="BlockPartnersController">
			<div class="col-md-12">
				<div class="splash" ng-cloak>
					<div class="image center-block"></div>
				</div>
				<div class="partners center-block">
				  <div ng-repeat="partner in partners" ng-cloak><a href="[[partner.url]]"><img src="{{ asset('[[partner.document.url]]') }}"/></a></div>
				</div>
			</div>
		</div>
    </div><!-- /.container -->
	<div class="footer-background"><img src="{{ asset('web/bundles/renovate/images/footer_renovate.png') }}"/></div>

